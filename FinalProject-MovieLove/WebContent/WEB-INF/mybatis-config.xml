<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases >
		<typeAlias type="spring.vo.bean.MemVO" alias="MemVO"/>
		<typeAlias type="spring.vo.bean.ReviewVO" alias="ReviewVO"/>
		<typeAlias type="spring.vo.bean.MovieVO" alias="MovieVO"/>
		<typeAlias type="spring.vo.bean.RankingVO" alias="RankingVO"/>
